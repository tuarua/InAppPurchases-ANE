<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/24.0">
    <id>com.tuarua.InAppPurchaseANE</id>
    <name>In App Purchases ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>1.8.0</versionNumber>
    <platforms>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.InAppPurchaseANE</initializer>
                <finalizer>com.tuarua.InAppPurchaseANE</finalizer>
            </applicationDeployment>
        </platform>
		<platform name="Android-ARM">
           <applicationDeployment>
               <nativeLibrary>classes.jar</nativeLibrary>
               <initializer>com.tuarua.InAppPurchaseANE</initializer>
               <finalizer>com.tuarua.InAppPurchaseANE</finalizer>
			</applicationDeployment>
        </platform>
		<platform name="Android-ARM64">
           <applicationDeployment>
               <nativeLibrary>classes.jar</nativeLibrary>
               <initializer>com.tuarua.InAppPurchaseANE</initializer>
               <finalizer>com.tuarua.InAppPurchaseANE</finalizer>
			</applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libInAppPurchaseANE.a</nativeLibrary>
                <initializer>TRIAPExtInizer</initializer>
                <finalizer>TRIAPExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libInAppPurchaseANE.a</nativeLibrary>
                <initializer>TRIAPExtInizer</initializer>
                <finalizer>TRIAPExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="appleTV-ARM">
            <applicationDeployment>
                <nativeLibrary>libInAppPurchaseANE.a</nativeLibrary>
                <initializer>TRIAPExtInizer</initializer>
                <finalizer>TRIAPExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="MacOS-x86-64">
            <applicationDeployment>
                <nativeLibrary>InAppPurchaseANE.framework</nativeLibrary>
                <initializer>TRIAPExtInizer</initializer>
                <finalizer>TRIAPExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
